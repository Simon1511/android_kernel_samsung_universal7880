	fimc_is_module_2p8: fimc-is_sensor_2p8@5A {
		compatible = "samsung,sensor-module-2p8";

		pinctrl-names = "pin0", "pin1";
		pinctrl-0 = <&fimc_is_mclk0_in &fimc_is_flash>;
		pinctrl-1 = <&fimc_is_mclk0_fn &fimc_is_flash>;

		position = <0>; /* Rear:0. Front:1 */
		id = <0>; /* sensor node id */
		mclk_ch = <0>;
		sensor_i2c_ch = <0>; /* I2C 0 */
		sensor_i2c_addr = <0x5A>;

		gpio_reset = <&gpf3 3 0x1>; /* sensor reset */
		gpio_dovdd_en = <&gpd3 7 0x1>; /* REAR_DOVDD_1P8 enable pin */
		status = "okay";

		/* TODO : need to update */
		af {
			product_name = <7>;
			i2c_addr = <0x18>;
			i2c_ch = <0>; /* I2C 0 */
		};

		/* TODO : need to update */
		flash {
			product_name = <11>; /* FLASH_GPIO */
			flash_first_gpio = <2>;
			flash_second_gpio = <1>;
		};
	};

	fimc_is_module_2p8_front: fimc-is_sensor_2p8_front@5A {
		compatible = "samsung,sensor-module-2p8";

		pinctrl-names = "pin0", "pin1";
		pinctrl-0 = <&fimc_is_mclk1_in>;
		pinctrl-1 = <&fimc_is_mclk1_fn>;

		position = <1>; /* Rear:0. Front:1 */
		id = <1>; /* sensor node id */
		mclk_ch = <1>;
		sensor_i2c_ch = <1>; /* I2C 0 */
		sensor_i2c_addr = <0x5A>;

		gpio_reset = <&gpf3 1 0x1>;
		gpio_dovdd_en = <&gpd3 3 0x1>; /* FRONT_DOVDD_1P8 enable pin */
		gpio_dvdd_en = <&gpd3 6 0x1>; /* FRONT_DVDD_1P2 enable pin */
		gpio_af_en = <&gpd3 2 0x1>; /* FRONT_CAM_AF_2P8 enable pin */
		status = "okay";
	};

