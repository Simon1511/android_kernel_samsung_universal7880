		flash-gpio = <&gpf4 2 0>;
		torch-gpio = <&gpf4 3 0>;
	};

	hsi2c_3: hsi2c@138A0000 {
		gpios = <&gpf0 0 0 &gpf0 1 0>;
		status = "okay";
		clock-frequency = <400000>;

		pinctrl-names = "default";
		pinctrl-0 = <&hs_i2c3_bus>;

		fimc-is-2p8@5A {
			compatible = "samsung,exynos5-fimc-is-cis-2p8";
			reg = <0x2D>;
			id = <0>; /* matching sensor id */
			setfile = "default";
		};

		fimc-is-actuator@18 {
			compatible = "samsung,exynos5-fimc-is-actuator-ak7345";
			reg = <0x0C>;
			id = <0>; /* matching sensor id */
		};
	};

	hsi2c_5: hsi2c@138C0000 {
		gpios = <&gpf0 3 0 &gpf0 2 0>;
		status = "okay";
		clock-frequency = <400000>;

		pinctrl-names = "default";
		pinctrl-0 = <&hs_i2c5_bus>;

		fimc-is-2p8@5A {
			compatible = "samsung,exynos5-fimc-is-cis-2p8-front";
			reg = <0x2D>;
			id = <1>; /* matching sensor id */
			setfile = "default";
		};
	};

